
@model List<U18OCT09X1.Models.Car>

<ul>
    @foreach (var item in Model)
    {
        <li>
            @item.Registration | @Html.ActionLink("info", "details", new { Registration = item.Registration }) | 
            <button type="button" class="btn btn-outline-info btn-sm" data-toggle="modal" data-target=".ModalRent">
             Rent
           </button>
        </li>
    }

</ul>
